{
  "check-coverage": true,
  "lines": 50,
  "functions": 50,
  "branches": 50,
  "exclude": [
    "**/*.d.ts",
    "src/internal/Binding.CopyOnWrite*.ts"
  ],
  "include": [
    "src/**/*.ts",
    "tests/**/*.ts"
  ],
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "reporter": [
    "lcov"
  ],
  "temp-dir": "coverage/.output",
  "report-dir": "coverage",
  "cache": false,
  "all": true
}
